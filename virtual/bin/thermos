#!/home/mwangi/moringa/thermos-cli/virtual/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'thermos','console_scripts','thermos'
__requires__ = 'thermos'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('thermos', 'console_scripts', 'thermos')()
    )
